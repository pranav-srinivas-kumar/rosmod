/*
 * ROSMOD Deployment Model
 */

// ROSMOD Hardware Model - cluster
using cluster;

// ROSMOD Node - Trajectory_Planning_Node 
node Trajectory_Planning_Node
{
  properties
  {
    ref = "cluster/BBB_112";
    priority = 50;
  }
  component_instance Sensor_i
  {
      properties
      {
	ref = "trajectory_planning_package/Sensor";
	scheduling_scheme = FIFO;
	logging
	{
	  DEBUG = true;
	  INFO = true;
	  WARNING = true;
	  ERROR = true;
	  CRITICAL = true;
	}
      }
  }
  component_instance Trajectory_Planner_i
  {
      properties
      {
	ref = "trajectory_planning_package/Trajectory_Planner";
	scheduling_scheme = FIFO;
	logging
	{
	  DEBUG = true;
	  INFO = true;
	  WARNING = true;
	  ERROR = true;
	  CRITICAL = true;
	}
      }
  }
}


