/*
 * ROSMOD Deployment Model
 */

// ROSMOD Hardware Model - cluster
using cluster;

// ROSMOD Node - Timer_Node 
node Timer_Node
{
  properties
  {
    ref = "cluster/BBB_111";
    priority = 50;
  }
  component_instance Timer_i
  {
      properties
      {
	ref = "timer_package/Timer";
	scheduling_scheme = FIFO;
	logging
	{
	  DEBUG = true;
	  INFO = true;
	  WARNING = true;
	  ERROR = true;
	  CRITICAL = true;
	}
      }
  }
}


