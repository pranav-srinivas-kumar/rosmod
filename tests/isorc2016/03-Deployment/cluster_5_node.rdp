/*
 * ROSMOD Deployment Model
 */

// ROSMOD Hardware Model - cluster
using cluster;

// ROSMOD Node - pubB_321 
node pubB_321
{
  properties
  {
    ref = "cluster/BBB_321";
    priority = 80;
    cmd_args = "--tg_time 50 --max_data_length_bits 160";		   
  }
  component_instance pubB
  {
      properties
      {
	ref = "isorc/pubB";
	scheduling_scheme = FIFO;
	logging
	{
	  Is Periodic? = true;
	  Periodic Log Unit = 1000;
	}
      }
  }
}
