/*
 * ROSMOD Deployment Model
 */

// ROSMOD Hardware Model - cluster
using cluster;

// ROSMOD Node - ISORC_Publisher 
node ISORC_Publisher
{
  properties
  {
    ref = "cluster/BBB_321";
    priority = 50;
  }
  component_instance senderBCDE
  {
      properties
      {
	ref = "multi_node_sample/senderBCDE";
	scheduling_scheme = PFIFO;
	logging
	{
	  Is Periodic? = true;
	  Periodic Log Unit = 1000;
	}
      }
  }
}
