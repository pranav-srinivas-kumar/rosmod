/*
 * ROSMOD Deployment Model
 */

// ROSMOD Hardware Model - cluster
using cluster;

// ROSMOD Node - PublisherNode_313 
node PublisherNode_313
{
  properties
  {
    ref = "cluster/BBB_313";
    priority = 50;
    cmd_args = "--tg_time 36 --max_data_length_bits 1600 --multiplier 0.3";		   
  }
  component_instance sender
  {
      properties
      {
	ref = "simple_pub_sub/sender";
	scheduling_scheme = FIFO;
	logging
	{
	  Is Periodic? = true;
	  Periodic Log Unit = 1000;
	}
      }
  }
}
// ROSMOD Node - SubscriberNode_314 
node SubscriberNode_314
{
  properties
  {
    ref = "cluster/BBB_314";
    priority = 50;
    cmd_args = "--tg_time 36";		   
  }
  component_instance receiver
  {
      properties
      {
	ref = "simple_pub_sub/receiver";
	scheduling_scheme = FIFO;
	logging
	{
	  Is Periodic? = true;
	  Periodic Log Unit = 1000;
	}
      }
  }
}


