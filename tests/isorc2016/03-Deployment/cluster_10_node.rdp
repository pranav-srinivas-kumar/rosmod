/*
 * ROSMOD Deployment Model
 */

// ROSMOD Hardware Model - cluster
using cluster;

// ROSMOD Node - NodeA_pub 
node NodeA_pub
{
  properties
  {
    ref = "cluster/BBB_321";
    priority = 50;
    cmd_args = "--tg_time 50 --max_data_length_bits 400";		   
  }
  component_instance senderBCDE
  {
      properties
      {
	ref = "multi_node_sample/senderBCDE";
	scheduling_scheme = PFIFO;
	logging
	{
	  Is Periodic? = true;
	  Periodic Log Unit = 1000;
	}
      }
  }
}
// ROSMOD Node - NodeA_sub 
node NodeA_sub
{
  properties
  {
    ref = "cluster/BBB_321";
    priority = 50;
    cmd_args = "--tg_time 50 --max_data_length_bits 400";		   
  }
  component_instance receiverA
  {
      properties
      {
	ref = "multi_node_sample/receiverA";
	scheduling_scheme = PFIFO;
	logging
	{
	  Is Periodic? = true;
	  Periodic Log Unit = 1000;
	}
      }
  }
}
