/*
 * ROSMOD Deployment Model
 */

// ROSMOD Hardware Model - cluster
using cluster;

// ROSMOD Node - NodeA_pub 
node NodeA_pub
{
  properties
  {
    ref = "cluster/BBB_321";
    priority = 50;
    cmd_args = "--tg_time 50 --max_data_length_bits 400";		   
  }
}
// ROSMOD Node - NodeA_sub 
node NodeA_sub
{
  properties
  {
    ref = "cluster/BBB_321";
    priority = 50;
    cmd_args = "--tg_time 50 --max_data_length_bits 400";		   
  }
}
