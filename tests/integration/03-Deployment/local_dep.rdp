/*
 * ROSMOD Deployment Model
 */

// ROSMOD Hardware Model - local
using local;

// ROSMOD Node - starter 
node starter
{
  properties
  {
    ref = "local/localhost";
    priority = 80;
    cmd_args = "--start --tg_time 50 --max_data_length_bits 800";		   
  }
  component_instance pubsub
  {
      properties
      {
	ref = "pubsub/comm";
	scheduling_scheme = FIFO;
	logging
	{
	  Is Periodic? = true;
	  Periodic Log Unit = 1000;
	}
      }
  }
}


