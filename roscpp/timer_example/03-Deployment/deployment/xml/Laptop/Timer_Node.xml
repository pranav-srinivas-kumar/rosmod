<?xml version="1.0"?>
<node name="Timer_Node">
  <component_instance name="Timer_Instance">
    <library name="libTimer.so"/>
    <scheduling_scheme setting="FIFO"/>
    <numCompsToSync setting="1"/>
    <syncTimeout setting="100.0"/>
    <logging>
      <is_periodic setting="True"/>
      <periodic_log_unit setting="1000"/> 
      <debug setting="False"/>
      <info setting="False"/>
      <warning setting="False"/>
      <error setting="False"/>
      <critical setting="False"/>
    </logging>
  </component_instance>
</node>

