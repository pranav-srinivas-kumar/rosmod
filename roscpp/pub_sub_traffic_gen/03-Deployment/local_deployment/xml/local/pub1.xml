<?xml version="1.0"?>
<node name="pub1">
  <library name="libnetwork_middleware.so"/>
  <component_instance name="sender_i">
    <library name="libsender.so"/>
    <scheduling_scheme setting="FIFO"/>
    <logging>
      <debug setting="True"/>
      <info setting="True"/>
      <warning setting="True"/>
      <error setting="True"/>
      <critical setting="True"/>
    </logging>
    <port_instance name="oob_pub1">
      <port name="oob_server"/>
      <group setting="pub1"/>
    </port_instance>
    <numCompsToSync setting="4"/>
    <syncTimeout setting="100.0"/>
    <profileName name="required1.csv"/>
    <tg_time setting="30.0"/>
  </component_instance>
</node>

