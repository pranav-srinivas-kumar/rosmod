/*
 * ROSMOD Deployment Model
 */

// ROSMOD Hardware Model - hardware
using hardware;

// ROSMOD Node - n1 
node n1
{
  properties
  {
    ref = "hardware/local";
    priority = 50;
  }
  component_instance comp_1_i
  {
      properties
      {
	ref = "pkg/comp_1";
	scheduling_scheme = FIFO;
	logging
	{
	  DEBUG = true;
	  INFO = true;
	  WARNING = true;
	  ERROR = true;
	  CRITICAL = true;
	}
      }
  }
}
