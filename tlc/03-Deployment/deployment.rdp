    ref = "hardware/laptop";
    priority = 50;
    cmd_args = "--id tl1443088101 --nsgreen GGGggrrrrGGGggrrrr --wegreen rrrrrGGggrrrrrGGgg --north e3_tl1443088101_19463160_12 --south e3_tl1443088101_19463160_11 --east e3_tl1443088101_19456179_0 --west e3_tl1443088101_19479801_10";		   
  }
  component_instance ctrl_i
