/*
 * ROS Deployment Model
 */

using Hardware;

// ROS Host Instance - BBB_000
host_instance BBB_000
{
    properties
    {
        username = "ubuntu";
	sshkey = "~/.ssh/id_rsa_jetsontk1";
    }

    nodes
    {
        // ROS Node Instance - satFlightApp_000 
        node_instance satFlightApp_000
	{
	    reference = "satellite_flight_application/satelliteFlightNode";
    	    cmdline_arguments = "sat000";		   
	}
    }
}

