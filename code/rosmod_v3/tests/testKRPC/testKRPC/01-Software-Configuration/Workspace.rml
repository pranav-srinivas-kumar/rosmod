/*
 * ROS Workspace Model
 */

// ROS Package - krpc
package krpc
{


    // Set of ROS Components
    components
    {
        // ROS Component - krpc_comp
	component krpc_comp
	{
	    timer timer0
	    {
	        period = 1.0s;
	    }
	}
    }

    // Set of ROS Nodes in this package
    nodes
    {
        node krcp_node 
	{
            // Instantiating components in ROS node
     	    component<krpc_comp> krcp_comp_inst;
	}
    }
}
