/*
 * ROSMOD Software Model
 */

// ROSMOD Package - krpc
package krpc
{
    // ROSMOD Component - krpc_comp
    component krpc_comp : base 
    {
        properties
        {
	    scheduling_scheme = PFIFO;
        }
        // ROSMOD Server - my_server
        server <krpc/some_srv> my_server 
        {
            priority = 60;
            deadline = 0.0001;
        }       
        // ROSMOD Publisher - my_publisher
        publisher <krpc/some_msg> my_publisher;
        // ROSMOD Timer - timer0
        timer timer0 
        {
            period = 1.0;
            priority = 55;
            deadline = 0.0002;
        }       
    }
    // ROSMOD Node - krcp_node
    node krcp_node 
    {
        properties
        {
            priority = 55;
        }
        // ROSMOD Component Instance - krcp_comp_inst
        component<krpc/krpc_comp> krcp_comp_inst;
    }
}
