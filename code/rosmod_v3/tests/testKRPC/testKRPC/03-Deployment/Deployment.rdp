/*
 * ROSMOD Deployment Model
 */

// ROSMOD Hardware Model - Hardware
using Hardware;

// ROSMOD Node - krcp_node_111 
node krcp_node_111
{
  properties
  {
    ref = "Hardware/BeagleboneBlack";
    priority = 55;
  }
  component_instance krpc_comp_inst
  {
      properties
      {
	ref = "krpc/krpc_comp";
	scheduling_scheme = PFIFO;
	logging
	{
	  DEBUG = false;
	  INFO = true;
	  WARNING = false;
	  ERROR = true;
	  CRITICAL = true;
	}
      }
      port_instance some_name 
      {
	ref = "krpc_comp/my_server";
	group = "G1";
      }
      port_instance some_name_2 
      {
	ref = "krpc_comp/my_publisher";
	group = "G563";
      }
  }
}


