// ROSMOD Hardware Model - Hardware
using Hardware;

// ROSMOD Hardware Instance - BBB_111
hardware_instance BBB_111
{
    properties
    {
        ref = "Hardware/BeagleboneBlack";
        ip_address = "10.1.1.1";
        username = "kelsier";
