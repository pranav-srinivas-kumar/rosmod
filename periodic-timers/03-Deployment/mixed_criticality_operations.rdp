/*
 * ROSMOD Deployment Model
 */

// ROSMOD Hardware Model - cluster
using cluster;

// ROSMOD Node - All_Timers_Node_312 
node All_Timers_Node_312
{
  properties
  {
    ref = "cluster/BBB_312";
    priority = 50;
  }
  component_instance All_Timers
  {
      properties
      {
	ref = "periodic_timers/All_Timers";
	scheduling_scheme = FIFO;
	logging
	{
	  Is Periodic? = true;
	  Periodic Log Unit = 1000;
	}
      }
  }
}


