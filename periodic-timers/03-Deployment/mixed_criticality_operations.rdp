// ROSMOD Hardware Model - cluster
using cluster;

// ROSMOD Node - All_Timers_Node_211 
node All_Timers_Node_211
{
  properties
  {
    ref = "cluster/BBB_211";
    priority = 50;
  }
  component_instance All_Timers_211
  {
      properties
      {
	ref = "periodic_timers/All_Timers";
	scheduling_scheme = PFIFO;
	logging
	{
	  Is Periodic? = true;
	  Periodic Log Unit = 1000;
	}
      }
  }
}


